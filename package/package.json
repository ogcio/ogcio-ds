  "author": {
    "name": "GOV.IE Design System Team (Office of the Government Chief Information Officer)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ogcio/ogcio-ds.git"
  },
  "bugs": {
    "url": "https://github.com/ogcio/ogcio-ds/issues"
  },
  "homepage": "https://ogcio.github.io/ogcio-ds/",
  "keywords": [
    "govie",
    "frontend",
    "design system",
    "template"
  ],

{
  "private": true,
  "name": "govie-frontend-repository",
  "description": "Used only for the development of GOV.IE Frontend, see `package/package.json` for the published `package.json`",
  "engines": {
    "node": "^16.13.0",
    "npm": "^8.1.0"
  },
  "license": "MIT",
  "workspaces": [
    "docs/examples/*",
    "package"
  ],
  "scripts": {
    "postinstall": "npm ls --depth=0",
    "storybook": " npm run build:dist:storybook && npm run storybook:ci",
    "storybook:ci": "start-storybook -p 6006",
    "storybook:build": "build-storybook --output-dir build",
    "build-release": "./bin/build-release.sh",
    "publish-release": "./bin/publish-release.sh",
    "pre-release": "./bin/pre-release.sh",
    "build:compile": "gulp compile",
    "build:sassdoc": "sassdoc --config sassdoc.config.yaml ./src/govie",
    "build:jsdoc": "jsdoc --configure jsdoc.config.js ./src/govie",
    "build:dist": "gulp build:dist --destination 'dist' && npm run test:build:dist",
    "build:dist:storybook": "gulp build:dist:storybook --destination 'storybook/dist'",
    "build:package": "gulp build:package --destination 'package' && npm run test:build:package",
    "test": "jest --color --ignoreProjects \"Gulp tasks\" --maxWorkers=50%",
    "test:build:package": "npx jest --selectProjects \"Gulp tasks\" --testMatch \"**/*build-package*\"",
    "test:build:dist": "npx jest --selectProjects \"Gulp tasks\" --testMatch \"**/*build-dist*\"",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --cache --cache-location .cache/eslint --cache-strategy content --color --ignore-path .gitignore --max-warnings 0 \"**/*.{cjs,js,mjs}\"",
    "lint:scss": "stylelint --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore --max-warnings 0 \"{app,src}/**/*.scss\""
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@etchteam/storybook-addon-css-variables-theme": "^1.4.0",
    "@percy/cli": "^1.12.0",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-docs": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/html": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "autoprefixer": "^9.8.8",
    "babel-jest": "^29.2.1",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.11",
    "del": "^6.1.1",
    "eslint": "^8.26.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.24",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.8",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "front-matter": "^4.0.2",
    "glob": "^8.0.3",
    "govuk_frontend_toolkit": "^9.0.1",
    "govuk_template_jinja": "^0.26.0",
    "govuk-elements-sass": "3.1.3",
    "gulp": "^4.0.2",
    "gulp-better-rollup": "3.1.0",
    "gulp-if": "^3.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-task-listing": "^1.1.1",
    "gulp-uglify": "^3.0.2",
    "html-validate": "7.5.0",
    "html5shiv": "^3.7.3",
    "jest": "^29.2.1",
    "jest-axe": "^6.0.0",
    "jest-dev-server": "^6.1.1",
    "jest-environment-jsdom": "^29.2.1",
    "jest-serializer-html": "^7.1.0",
    "jquery": "1.12.4",
    "js-beautify": "^1.14.7",
    "js-yaml": "^4.1.0",
    "jsdoc": "^3.6.11",
    "jsdoc-tsimport-plugin": "^1.0.5",
    "map-stream": "^0.0.7",

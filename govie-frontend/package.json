    "react-code-blocks": "^0.0.9-0",
    "react-dom": "^17.0.2",
    "react18": "npm:react@^18.0.0",
    "recursive-readdir": "^2.2.2",
    "rollup": "0.56.5",
    "sass-color-helpers": "^2.1.1",
    "sassdoc": "^2.7.4",
    "shuffle-seed": "^1.1.6",
    "slash": "^3.0.0",
    "standard": "^17.0.0",
    "storybook-addon-themes": "^6.1.0",
    "stylelint": "^14.14.0",
    "stylelint-config-gds": "^0.2.0",
    "stylelint-order": "^5.0.0",
    "undici": "^5.11.0",
    "wait-on": "^6.0.1",
    "ws": "^8.9.0",
    "yargs": "^17.6.0"
  }
}

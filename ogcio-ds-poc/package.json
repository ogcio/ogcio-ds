{
  "name": "ogcio-ds",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "watch": "concurrently --names packages/built,packages/components \"npm run watch -w packages/built\" \"npm run watch -w packages/components\"",
    "test": "npm run test --workspaces --if-present",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook --output-dir build",
    "example:html": "npm run start -w examples/html",
    "example:cra": "npm run start -w examples/create-react-app",
    "prepare_": "husky install",
    "prettier": "prettier --write \"(packages|examples)/**/*.{css,scss,md}\"",
    "lint:fix": "eslint --fix \"packages/**/*.{js,jsx,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogcio/ogcio-ds.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ogcio/ogcio-ds/issues"
  },
  "homepage": "https://github.com/ogcio/ogcio-ds#readme",
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "babel-jest": "^29.0.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "8.22",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "storybook-addon-themes": "^6.1.0",
    "style-loader": "^3.3.1"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-dictionary": "^3.7.1"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "lint-staged": {
    "packages/**/*.{js,jsx,json}": [
      "eslint --fix"
    ],
    "packages/**/*.{css,scss,md}": [
      "prettier --write"
    ]
  }
}

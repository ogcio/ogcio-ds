{
  "private": true,
  "name": "ogcio-ds",
  "description": "Used only for the development of OGCIO-DS, see `package/package.json` for the published `package.json`",
  "engines": {
    "node": "^16.13.0",
    "npm": "^8.1.0"
  },
  "license": "MIT",
  "workspaces": [
    "package"
  ],
  "scripts": {
    "postinstall": "npm ls --depth=0",
    "storybook": " npm run build:storybook:dist && npm run storybook:ci",
    "storybook:ci": "start-storybook -p 6006",
    "storybook:build": "build-storybook --output-dir build",
    "build:storybook:dist": "gulp build:storybook:dist --destination 'storybook/dist'",
    "build-release": "./bin/build-release.sh",
    "publish-release": "./bin/publish-release.sh",
    "pre-release": "./bin/pre-release.sh",
    "build:compile": "gulp compile",
    "build:dist": "gulp build:dist --destination \"dist\"",
    "build:package": "gulp build:package --destination \"package\"",
    "test": "echo Tests skipped.",
    "test:tofix": "jest --color --ignoreProjects \"Gulp tasks\" --maxWorkers=50%",
    "test:build:package": "npx jest --selectProjects \"Gulp tasks\" --testMatch \"**/*build-package*\"",
    "test:build:dist": "npx jest --selectProjects \"Gulp tasks\" --testMatch \"**/*build-dist*\"",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --cache --cache-location .cache/eslint --cache-strategy content --color --ignore-path .gitignore --max-warnings 0 \"**/*.{cjs,js,mjs}\"",
    "lint:scss": "stylelint --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore --max-warnings 0 \"{app,src}/**/*.scss\"",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.{js,json,css,scss,html,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,json,css,scss,html,md}\""
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@etchteam/storybook-addon-css-variables-theme": "^1.4.0",
    "@percy/cli": "^1.12.0",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-docs": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/html": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "autoprefixer": "^9.8.8",
    "babel-jest": "^29.4.2",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.11",
    "del": "^7.0.0",
    "eslint": "^8.26.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.24",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.8",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "front-matter": "^4.0.2",
    "glob": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-task-listing": "^1.1.1",
    "html-validate": "7.5.0",
    "html5shiv": "^3.7.3",
    "husky": "^8.0.0",
    "jest": "^29.4.3",
    "jest-axe": "^7.0.0",
    "jest-dev-server": "^7.0.0",
    "jest-environment-jsdom": "^29.4.3",
    "jest-environment-node-single-context": "^29.0.0",
    "jest-environment-puppeteer": "^7.0.1",
    "jest-serializer-html": "^7.1.0",
    "jquery": "1.12.4",
    "js-beautify": "^1.14.7",
    "js-yaml": "^4.1.0",
    "jsdoc": "^3.6.11",
    "jsdoc-tsimport-plugin": "^1.0.5",
    "map-stream": "^0.0.7",
    "marked": "^4.1.1",
    "merge-stream": "^2.0.0",
    "minimatch": "^6.2.0",
    "node-sass": "^7.0.3",
    "nodemon": "^2.0.20",
    "outdent": "^0.8.0",
    "plugin-error": "^2.0.1",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "postcss-pseudo-classes": "^0.2.1",
    "postcss-scss": "^4.0.6",
    "postcss-unmq": "^1.0.2",
    "postcss-unopacity": "^2.0.0",
    "postcss-unrgba": "^1.1.1",
    "prettier": "^3.2.5",
    "react": "^17.0.2",
    "react-code-blocks": "^0.0.9-0",
    "react-dom": "^17.0.2",
    "react18": "npm:react@^18.0.0",
    "recursive-readdir": "^2.2.2",
    "rollup": "0.59.4",
    "sass-color-helpers": "^2.1.1",
    "sassdoc": "^2.7.4",
    "shuffle-seed": "^1.1.6",
    "slash": "^5.0.0",
    "standard": "^17.0.0",
    "storybook-addon-themes": "^6.1.0",
    "stylelint": "^14.14.0",
    "stylelint-config-gds": "^0.2.0",
    "stylelint-order": "^5.0.0",
    "terser": "^5.16.3",
    "undici": "^5.11.0",
    "wait-on": "^6.0.1",
    "ws": "^8.9.0",
    "yargs-parser": "^21.1.1"
  },
  "overrides": {
    "chokidar@^2": {
      "glob-parent": "^5"
    },
    "glob-stream@^6": {
      "glob-parent": "^5"
    },
    "latest-version@^5": {
      "package-json": "^7"
    },
    "postcss-unmq": {
      "postcss": "^7"
    },
    "postcss-unopacity": {
      "postcss": "^7"
    },
    "postcss-unrgba": {
      "postcss": "^7"
    },
    "sass-convert": {
      "semver-regex": "^3"
    },
    "sassdoc-extras@^2": {
      "marked": "^0.8.2"
    }
  },
  "version": "0.1.0"
}
